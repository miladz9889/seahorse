// Jupiter Terminal Integration
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta
      http-equiv="Content-Security-Policy"
      content="script-src 'self' https://terminal.jup.ag 'unsafe-eval'; object-src 'none';"
    />
    <title>Jupiter Terminal Example --gead- work</title>
    <script src="https://terminal.jup.ag/main-v3.js" defer></script>
  </head>
  <body>
    <h1>Buy a Project Token</h1>
    <div id="jupiter-terminal" style="width: 400px; height: 568px;"></div>
    <script>
      window.Jupiter.init({
        displayMode: "integrated",
        integratedTargetId: "jupiter-terminal",
        endpoint: "src/components/jupiter-terminal.astro",
        strictTokenList: false,
        defaultExplorer: "Solscan",
        formProps: {
          fixedOutputMint: true,
          swapMode: "ExactIn",
          initialOutputMint: "7L6M32mpkewGWj8bHspoX8QDe9pb3b3S1U33KpFvpump", // Replace with your token mint address
        },
      });
    </script>
  </body>
</html>